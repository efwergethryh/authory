            <form onclick="event.stopPropagation();" class="add-conversation" id="add_conversation" action=""
                enctype="multipart/form-data">
                <i class="fa"></i>
                <h3>Create a new conversation</h3>
                <input type="text" onclick="event.stopPropagation(); " id="title" name="title"
                    placeholder="Enter conversation name" required>
                <div class="avatar-upload">
                    <input type="file" hidden id="conversation_picture" name="conv_pic" accept="image/*" />
                    <label for="conversation_picture" id="avatar-label" class="avatar-button">
                        <div id="avatar-preview" class="avatar-preview"></div>
                    </label>
                </div>

                <input id="user-name" name="user_name" type="text" placeholder="Enter a member to add" required>
                <ul id="users_dropdown" class="users_dropdown"></ul>
                <button id="add-conv">Create</button>
            </form>
            <div id="maincontent" class="mainContent shifted">
            </div>

            <div id="options-popup" style="display: none;" class="options-popup">
                <div class="options-popup-content">
                    <ul>
                        <label for="image-input">
                            <a>
                                <i class="fa-solid fa-camera"></i>
                                <p>photos</p>
                            </a>
                        </label>
                        <input type="file" id="image-input" style="display: none;" name="file" accept="image/*" />

                        <label for="file-input">
                            <a>
                                <i class="fa-regular fa-file"></i>
                                <p>Document</p>
                            </a>
                        </label>
                        <input type="file" id="file-input" style="display: none;" name="file"
                            accept=".pdf,.doc,.docx,.txt,.xls,.xlsx,.ppt,.pptx" />
                    </ul>

                </div>
            </div>

            <div id="paper-settings" class="paper-settings">
                <ul>
                    <a id="editPaper">
                        <i class="fa-solid fa-pen"></i>
                        <p>Edit paper</p>
                    </a>
                    <a id="delete-user-from-paper">
                        <i class="fa-solid fa-user-xmark"></i>
                        <p>Delete a user from paper</p>
                    </a>
                    <a id="deletePaper">
                        <i class="fa-solid fa-trash" style="color: red;"></i>
                        <p style="color: red;">Delete paper</p>
                    </a>
                </ul>
            </div>
            <div id="conversation-settings" class="paper-settings">
                <ul>
                    <a id="editPaper">
                        <i class="fa-solid fa-pen"></i>
                        <p>Remove members</p>
                    </a>
                    
                </ul>
            </div>
            <div id="users-to-delete" class="users-to-delete">
                
            </div>
            
            <div id="conversation-users" class="users-to-delete">
                <i style="top: 0;" id="close-button" class="fa-solid fa-xmark close-button"></i>
            </div>
            
            <div id="confirm-delete" class="confirm-delete">
                <p>Do you want to delete this paper?</p>
                <div class="buttons">
                    <a id="cancel">cancel</a>
                    <a onclick="delete_paper()" id="confirm">confirm</a>
                </div>
            </div>
            <div class="edit-paper">

            </div>
            <img src="/images/Book.gif" class="loading-spinner" id="loading-spinner" alt="">
            <div id="userProfile" class="userProfile">

            </div>
            <div id="chat-filter" class="chat-filter">
                <p>Chat filter</p>
                <input id="filter-input" placeholder="select filter">

            </div>
            <ul class="filter-select" id="filter-select">
                <li value="">All</li>
                <li value="Engineering">Engineering</li>
                <li value="Medicine and Health Sciences">Medicine and Health Sciences</li>
                <li value="Computer Science">Computer Science</li>
                <li value="Law">Law</li>
                <li value="Natural Sciences">Natural Sciences</li>
                <li value="Social and Humanities Sciences">Social and Humanities Sciences</li>
                <li value="Economics and Management">Economics and Management</li>
                <li value="Agriculture and Natural Resources">Agriculture and Natural Resources</li>
                <li value="Arts">Arts</li>
                <li value="Material Sciences">Material Sciences</li>
                <li value="Space Sciences">Space Sciences</li>
                <li value="Interdisciplinary Studies">Interdisciplinary Studies</li>
                <li value="Environmental and Sustainability">Environmental and Sustainability</li>
                <li value="Security Studies">Security Studies</li>
                <li value="Marine Sciences">Marine Sciences</li>
                <li value="Disaster Management">Disaster Management</li>
            </ul>

